<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <!-- <key>SessionCreate</key><true/> -->
    <key>KeepAlive</key>
    <dict>
      <key>SuccessfulExit</key><true/>
    </dict>
    <key>RunAtLoad</key><true/>
    <key>Disabled</key><false/>
    <key>Label</key>
    <string>{{ runner_service_label }}</string>
    <key>UserName</key>
    <string>{{ runner_user }}</string>
    <key>GroupName</key>
    <string>{{ runner_user }}</string>
    <key>Program</key>
    <string>/Users/{{ runner_user }}/bin/gitlab-runner</string>
    <key>ProgramArguments</key>
    <array>
      <string>run</string>
      <string>--working-directory</string>
      <string>/Users/{{ runner_user }}/work</string>
      <string>--syslog</string>
      <string>--debug</string>
    </array>
    <key>WorkingDirectory</key>
    <string>/Users/{{ runner_user }}</string>
    <key>EnvironmentVariables</key>
    <dict>
      <key>PATH</key>
      <string>/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin</string>
    </dict>
    <!--
    <key>ProcessType</key>
    <string>Background</string>
    -->
  </dict>
</plist>
